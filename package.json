{
  "name": "art-grade",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install && chmod +x .husky/*",
    "commit": "git-cz",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check --ignore-unknown ."
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "eslint",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@paypal/react-paypal-js": "^8.1.3",
    "@reduxjs/toolkit": "^1.9.7",
    "@stripe/react-stripe-js": "^2.3.2",
    "@stripe/stripe-js": "^2.1.11",
    "antd": "^5.11.3",
    "apexcharts": "^3.44.1",
    "chart.js": "^4.4.0",
    "classnames": "^2.3.2",
    "formik": "^2.4.5",
    "js-cookie": "^3.0.5",
    "moment": "^2.29.4",
    "next": "14.0.1",
    "react": "^18",
    "react-apexcharts": "^1.4.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18",
    "react-infinite-scroll-component": "^6.1.0",
    "react-quill": "^2.0.0",
    "react-redux": "^8.1.3",
    "react-share": "^5.0.3",
    "react-simple-star-rating": "^5.1.7",
    "sharp": "^0.33.2",
    "slate": "^0.101.1",
    "slate-history": "^0.100.0",
    "slate-react": "^0.101.3",
    "stripe": "^14.4.0",
    "sweetalert2": "^11.9.1",
    "swiper": "^11.0.4",
    "usehooks-ts": "^2.9.1",
    "yup": "^1.3.3",
    "yup-phone": "^1.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@types/js-cookie": "^3.0.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "daisyui": "^3.9.4",
    "eslint": "^8.53.0",
    "eslint-config-next": "14.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
